<div class="page-header">
  <h1>
    Available hours
    <a href="/available_hours/new" class="btn btn-lg btn-success">New Available hour</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Provider</th>
        <th>Time</th>
        <th>Actions</th>
      </tr>

      <% @available_hours.each do |available_hour| %>
      <tr>
        <td><%= available_hour.provider %></td>
        <td><%= available_hour.time %></td>
        <td>
          <a href="/available_hours/<%= available_hour.id %>" class="btn btn-primary">Show</a>
          <a href="/available_hours/<%= available_hour.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_available_hour/<%= available_hour.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
